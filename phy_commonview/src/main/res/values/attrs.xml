<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ReadAndStopBt">
        <attr name="android:layout_width"  format="dimension">
                <enum name="fill_parent" value="-1" />
                <enum name="match_parent" value="-1" />
                <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="android:layout_height"   format="dimension">
                <enum name="fill_parent" value="-1" />
                <enum name="match_parent" value="-1" />
                <enum name="wrap_content" value="-2" />
        </attr>
        <attr format="float" name="layout_constraintHeight_percent"/>
        <attr format="float" name="layout_constraintVertical_bias"/>

    </declare-styleable>

    <declare-styleable name="TopNavigationBar">
        <attr name="title" format="string"/>
        <attr name="SecondTitle" format="string"/>
        <attr name="rightTextWidget" format="string"/>
    </declare-styleable>


</resources>